<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".PreviewFoodActivity">

    <TextView
            android:text="@string/restaurant"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView6"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.036" android:textStyle="bold"/>
    <TextView
            android:text="@string/ratting"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView7"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/txtRestaurante" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.033" android:textStyle="bold"/>
    <TextView
            android:text="@string/description"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView8"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/rbNotaPreview" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.037" android:textStyle="bold"/>
    <TextView
            android:text="@string/address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView9"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="20dp" android:layout_marginStart="20dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/txtTelefone"
            android:textStyle="bold"/>
    <TextView
            android:text="@string/num_address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView10"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="68dp" android:layout_marginRight="68dp"
            app:layout_constraintStart_toEndOf="@+id/textView9" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.919"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/txtTelefone"
            android:textStyle="bold"/>
    <TextView
            android:text="@string/phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView11"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/txtDescricao" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.033" android:textStyle="bold"/>

    <fragment
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="465dp"
            android:layout_height="164dp"
            android:id="@+id/mvMapa"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.435" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/txtEndereco"/>


    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/txtRestaurante"
            android:drawableBottom="@color/colorAccent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView6" app:layout_constraintHorizontal_bias="0.035"/>
    <RatingBar
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/rbNotaPreview"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView7" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.077"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/txtDescricao"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView8" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.035"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/txtEndereco"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="20dp" android:layout_marginStart="20dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView9"
            app:layout_constraintEnd_toStartOf="@+id/txtNumEndereco" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.0"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/txtNumEndereco"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView10"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="92dp"
            android:layout_marginRight="92dp"
    />
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/txtTelefone"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView11" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="20dp" android:layout_marginStart="20dp"/>
    <Button
            android:text="@string/delete"
            android:layout_width="106dp"
            android:layout_height="wrap_content"
            android:id="@+id/btnDeletar"
            android:backgroundTint="@color/button_save"
            android:textColor="@color/button_text"
            android:drawableLeft="@drawable/ic_delete"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/mvMapa" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="32dp" android:layout_marginStart="32dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.106"/>
    <Button
            android:text="@string/edit"
            android:layout_width="106dp"
            android:layout_height="wrap_content"
            android:id="@+id/btnEditar"
            android:backgroundTint="@color/button_save"
            android:textColor="@color/button_text"
            android:drawableLeft="@drawable/ic_edit"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/mvMapa" app:layout_constraintStart_toEndOf="@+id/btnDeletar"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.106"/>
    <Button
            android:text="@string/shared"
            android:layout_width="106dp"
            android:layout_height="wrap_content"
            android:id="@+id/btnShared"
            android:onClick="sharedFoodPermission"
            android:backgroundTint="@color/button_save"
            android:textColor="@color/button_text"
            android:drawableLeft="@drawable/ic_share"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/mvMapa"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp"
            app:layout_constraintStart_toEndOf="@+id/btnEditar" android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp" app:layout_constraintHorizontal_bias="0.241"
            app:layout_constraintVertical_bias="0.106"/>
    <Button
            android:text="@string/call"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:id="@+id/btnLigar"
            android:textSize="8dp"
            android:backgroundTint="@color/button_save"
            android:textColor="@color/button_text"
            android:drawableLeft="@drawable/ic_call"
            app:layout_constraintStart_toEndOf="@+id/txtTelefone"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView11" android:textStyle="normal"
            app:layout_constraintHorizontal_bias="0.187"/>
</android.support.constraint.ConstraintLayout>